{
  "name": "@voiceflow/slate-serializer",
  "type": "module",
  "description": "Utilities for serializing slate data",
  "version": "1.5.5",
  "author": "Ben Teichman",
  "bugs": {
    "url": "https://github.com/voiceflow/slate-serializer/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "csstype": "3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.1",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/react": "18.2.4",
    "@voiceflow/base-types": "2.90.1",
    "@voiceflow/commitlint-config": "2.1.0",
    "@voiceflow/eslint-config": "7.1.0",
    "@voiceflow/git-branch-check": "1.4.0",
    "@voiceflow/prettier-config": "1.3.0",
    "@voiceflow/tsconfig": "1.4.8",
    "chai": "4.3.7",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "1.4.3",
    "eslint": "8.39.0",
    "eslint-output": "^3.0.1",
    "fixpack": "^4.0.0",
    "husky": "8.0.3",
    "istanbul": "^0.4.5",
    "lint-staged": "13.1.2",
    "mocha": "10.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "slate": "0.94.1",
    "source-map-support": "0.5.21",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.6",
    "tsc-esm-fix": "2.20.13",
    "typescript": "5.0.4"
  },
  "exports": {
    ".": {
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./text": {
      "import": "./build/esm/serializers/text.mjs",
      "require": "./build/cjs/serializers/text.cjs"
    },
    "./markdown": {
      "import": "./build/esm/serializers/markdown.mjs",
      "require": "./build/cjs/serializers/markdown.cjs"
    },
    "./jsx": {
      "import": "./build/esm/serializers/jsx.mjs",
      "require": "./build/cjs/serializers/jsx.cjs"
    }
  },
  "files": [
    "build/"
  ],
  "homepage": "https://github.com/voiceflow/slate-serializer#readme",
  "keywords": [
    "react",
    "slate",
    "voiceflow"
  ],
  "license": "ISC",
  "peerDependencies": {
    "@voiceflow/base-types": "^2.67.0",
    "react": "^17.0.2",
    "slate": "^0.72.3"
  },
  "prettier": "@voiceflow/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/slate-serializer.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.build.cjs.json && tsc-alias -p tsconfig.build.cjs.json && tsc-esm-fix --sourceMap --target build/cjs --ext .cjs",
    "build:esm": "tsc -p tsconfig.build.esm.json && tsc-alias -p tsconfig.build.esm.json && tsc-esm-fix --sourceMap --target build/esm --ext .mjs",
    "clean": "rimraf build",
    "commit": "cz",
    "eslint-output": "eslint-output",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:output": "yarn run eslint-output --quiet \"**/*.{js,ts,tsx}\"",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint:output",
    "prepublishOnly": "yarn build",
    "test": "yarn test:run",
    "test:dependencies": "depcheck",
    "test:run": "NODE_ENV=test nyc ts-mocha --paths --config ./config/test/.mocharc.yml 'test/**/*.unit.ts'",
    "test:single": "NODE_ENV=test ts-mocha --paths --config ./config/test/.mocharc.yml",
    "test:unit": "NODE_ENV=test nyc --report-dir=nyc_coverage_unit ts-mocha --paths --config ./config/test/.mocharc.yml 'test/**/*.unit.ts'"
  },
  "types": "build/esm/index.d.ts",
  "typesVersions": {
    "*": {
      "text": [
        "./build/esm/serializers/text.d.ts"
      ],
      "jsx": [
        "./build/esm/serializers/jsx.d.ts"
      ],
      "markdown": [
        "./build/esm/serializers/markdown.d.ts"
      ]
    }
  }
}
