{
  "name": "@voiceflow/eslint-config",
  "description": "ESLint config for voiceflow",
  "version": "6.1.0",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/voiceflow/eslint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "3.2.5",
    "eslint-plugin-compat": "4.0.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "eslint-plugin-unicorn": "40.0.0",
    "eslint-plugin-xss": "^0.1.9",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0"
  },
  "devDependencies": {
    "@voiceflow/commitlint-config": "2.0.0",
    "@voiceflow/git-branch-check": "1.2.3",
    "@voiceflow/prettier-config": "1.2.1",
    "commitizen": "^4.2.4",
    "commitlint": "16.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.6.0",
    "fixpack": "^4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "prettier": "2.7.1"
  },
  "files": [
    "*.js",
    "config/*.js",
    "overrides/*.js",
    "!scratch.*"
  ],
  "homepage": "https://github.com/voiceflow/eslint-config#readme",
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "@voiceflow/prettier-config": "^1.0.2",
    "eslint": "^8.0.0"
  },
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/eslint-config.git"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "5.30.6"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint \"./**/*.{js,ts}\"",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "prepare": "husky install",
    "test": "yarn test:run",
    "test:run": "echo \"Error: no test specified\" && exit 1"
  }
}
